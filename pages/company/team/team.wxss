.headetitlebox{
  width: 690rpx;
  margin: auto;
  margin-top: 30rpx;
  font-size: 36rpx;
  color: #14171F;
  line-height: 50rpx
}
.leftbar{
  width: 10rpx;
  height: 50rpx;
  background: #03B4AA;
  margin-right: 30rpx;
  
}
.threebox{
  width: 200rpx;
  height: 100rpx;
  /* border: 1rpx sienna solid; */
  color: #B5B5B5;
  font-size: 28rpx;
  text-align: center;
  line-height: 80rpx
}
.userheader{
  width: 90%;
  height: 150rpx;
  margin: auto;
  margin-top: 20rpx;
}
.img{
  height: 118rpx;
  width: 118rpx;
  position: relative;
  margin-top: 20rpx;
  margin-right: 30rpx;
}
.point{
  width: 25rpx;
  height: 25rpx;
  background: #03B4AA;
  border: 4rpx solid #f3fbf6;
  border-radius: 50%;
  position: absolute;
  bottom: 5rpx;
  right: 5rpx;
  z-index: 4;
  
}
.userheader image{
  height: 110rpx;
  width: 110rpx;
  display: block;
  border: 4rpx solid #fff;
  border-radius: 50%;
  margin-right: 30rpx;
}
.username{
  height: 100rpx;
  font-size: 30rpx;
  color:  #14171F;
  font-weight: bold;
  /* border: 1rpx solid red; */
  margin-top: 30rpx
}
.zhize{
  font-size: 28rpx;
  color: #5F626A;
  font-weight: normal;
  margin-top: 20rpx;
}
.yellow{
  width: 14rpx;
  height: 14rpx;
  background: #F7B664;
  border-radius: 50%;
  margin:13rpx 10rpx 0 0
}
.innerbox {
  height: 200rpx;
  display: flex;
  flex-direction: row;
  white-space: nowrap;
  align-items: center;
  position: relative;
  margin-top: 40rpx;
  box-sizing: border-box;
  padding: 0 20rpx
}
.imgitem {
  margin-left: 10rpx;
  display: inline-block;
  width:330rpx;
  height: 200rpx;
  flex-direction: column;
  
}
.contentbox{
  width: 350rpx;
  height: 230rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start
}
.imgsbox{
  width:320rpx;
  height: 180rpx;
  margin: auto;
  /* border: 1rpx sienna solid; */
  background: #fff;
  margin-top: 10rpx

}
.imgitem image{
  width: 330rpx;
  height: 180rpx;
  border-radius: 15rpx
}